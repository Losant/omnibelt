{
  "name": "losant-utils",
  "version": "1.0.0",
  "private": true,
  "main": "src/index.js",
  "repository": "git@bitbucket.org:losant/losant-utils.git",
  "author": "@Losant",
  "engines": {
    "node": "8.4.0",
    "yarn": "1.0.1"
  },
  "scripts": {
    "preinstall": "node -e 'if(!process.env.npm_execpath.includes(`yarn`)){console.error(`Use yarn!`);process.exit(1);}'",
    "lint": "esw *.js src",
    "lint:fix": "esw --fix *.js src",
    "lint:watch": "esw --watch *.js src",
    "test": "jest --verbose",
    "test:watch": "jest --watch -o --verbose"
  },
  "dependencies": {
    "lodash": "^4.17.4",
    "ramda": "^0.24.1"
  },
  "devDependencies": {
    "eslint-config-losant": "git+ssh://git@bitbucket.org/losant/eslint-config-losant.git#v1.1.24",
    "jest": "^20.0.4"
  },
  "eslintConfig": {
    "extends": "losant/env/node",
    "env": {
      "jest": true
    }
  }
}
