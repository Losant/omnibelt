{
  "name": "losant-utils",
  "version": "1.1.8",
  "private": true,
  "main": "src/index.js",
  "repository": "git@github.com/Losant/losant-utils.git",
  "author": "@Losant",
  "engines": {
    "node": ">=8.9.4",
    "yarn": ">=1.3.2"
  },
  "scripts": {
    "reinstall": "rm -rf node_modules && yarn install",
    "lint": "esw *.js src test",
    "lint:fix": "yarn lint --fix",
    "lint:watch": "yarn lint --watch",
    "lint:staged": "lint-staged",
    "test": "jest --verbose",
    "test:watch": "yarn test --watch -o",
    "precommit": "lint-staged",
    "postmerge": "losant-scripts git-hook post-merge",
    "postversion": "losant-scripts git-hook post-version",
    "docs": "yarn docs:open",
    "predocs:build": "./scripts/gen-3rd-party-docs.js",
    "docs:build": "jsdoc -c jsdoc.json",
    "docs:open": "open ./docs/index.html"
  },
  "lint-staged": {
    "*.js": "esw"
  },
  "dependencies": {
    "@rjhilgefort/export-dir": "^1.1.0",
    "lodash": "^4.17.4",
    "ramda": "^0.25.0",
    "string-format": "^0.5.0",
    "stringify-object": "^3.2.1"
  },
  "devDependencies": {
    "@losant/eslint-config-losant": "^1.3.2",
    "@losant/scripts": "^1.0.10",
    "docdash": "^0.4.0",
    "expect": "^22.4.3",
    "fs-extra": "^5.0.0",
    "handlebars": "^4.0.11",
    "husky": "^0.14.3",
    "jest": "^21.0.2",
    "jsdoc": "^3.5.5",
    "lint-staged": "^6.0.0",
    "present": "^1.0.0",
    "treis": "^2.5.0"
  },
  "eslintConfig": {
    "extends": "@losant/eslint-config-losant/env/node",
    "env": {
      "jest": true
    },
    "rules": {
      "arrow-body-style": 0,
      "valid-jsdoc": 0
    }
  },
  "eslintIgnore": [
    "docs"
  ]
}
