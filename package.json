{
  "name": "losant-utils",
  "version": "1.0.4",
  "private": true,
  "main": "src/index.js",
  "repository": "git@bitbucket.org:losant/losant-utils.git",
  "author": "@Losant",
  "engines": {
    "node": "8.7.0",
    "yarn": "^1.2.0"
  },
  "scripts": {
    "preinstall": "node -e 'if(!process.env.npm_execpath.includes(`yarn`)){console.error(`Use yarn!`);process.exit(1);}'",
    "lint": "esw *.js src",
    "lint:fix": "esw --fix *.js src",
    "lint:watch": "esw --watch *.js src",
    "lint:changed": "lint-staged",
    "test": "jest --verbose",
    "test:watch": "jest --watch -o --verbose",
    "precommit": "lint-staged"
  },
  "lint-staged": {
    "*.js": "esw"
  },
  "dependencies": {
    "lodash": "^4.17.4",
    "ramda": "^0.24.1"
  },
  "devDependencies": {
    "eslint-config-losant": "git+ssh://git@bitbucket.org/losant/eslint-config-losant.git#v1.1.27",
    "fs-extra": "^4.0.2",
    "husky": "^0.14.3",
    "jest": "^21.0.2",
    "lint-staged": "^4.1.3"
  },
  "eslintConfig": {
    "extends": "losant/env/node",
    "env": {
      "jest": true
    }
  }
}
