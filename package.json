{
  "name": "omnibelt",
  "description": "A utility belt that normalizes and wraps `Ramda` and `Lodash/FP`.",
  "version": "3.0.0",
  "license": "MIT",
  "main": "src/index.js",
  "repository": "git@github.com/Losant/omnibelt.git",
  "author": "@Losant",
  "contributors": [
    {
      "name": "Rob Hilgefort",
      "email": "rjhilgefort@gmail.com",
      "web": "rob.hilgefort.me"
    }
  ],
  "engines": {
    "node": ">=14"
  },
  "scripts": {
    "reinstall": "rm -rf node_modules && yarn install",
    "lint": "esw . --ext .js",
    "lint:fix": "yarn lint --fix",
    "lint:watch": "yarn lint --watch",
    "lint:staged": "lint-staged",
    "test": "jest --verbose",
    "test:watch": "yarn test --watch -o",
    "docs": "yarn docs:open",
    "predocs:build": "./scripts/gen-3rd-party-docs.js",
    "docs:build": "jsdoc -c jsdoc.json",
    "docs:open": "open ./docs/index.html",
    "prepare": "husky install"
  },
  "lint-staged": {
    "*.js": "esw"
  },
  "dependencies": {
    "@rjhilgefort/export-dir": "^2.0.0",
    "lodash": "^4.17.21",
    "ramda": "^0.28.0",
    "string-format": "^2.0.0",
    "stringify-object": "^3.3.0"
  },
  "devDependencies": {
    "@losant/eslint-config-losant": "1.5.0",
    "docdash": "^1.2.0",
    "expect": "^29.3.1",
    "fs-extra": "^10.1.0",
    "handlebars": "^4.7.7",
    "husky": "^8.0.2",
    "jest": "^29.3.1",
    "jsdoc": "^4.0.0",
    "lint-staged": "^13.0.3",
    "present": "^1.0.0",
    "taffydb": "^2.7.3",
    "treis": "^2.6.0"
  },
  "eslintConfig": {
    "extends": "@losant/eslint-config-losant/env/node",
    "env": {
      "jest": true
    },
    "rules": {
      "valid-jsdoc": 0
    }
  },
  "eslintIgnore": [
    "docs"
  ]
}
