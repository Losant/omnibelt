{
  "name": "omnibelt",
  "description": "A utility belt that normalizes and wraps `Ramda` and `Lodash/FP`.",
  "version": "1.2.0",
  "license": "MIT",
  "main": "dist/omnibelt.cjs.js",
  "module": "dist/omnibelt.esm.js",
  "repository": "git@github.com/Losant/omnibelt.git",
  "author": "@Losant",
  "contributors": [
    {
      "name": "Rob Hilgefort",
      "email": "rjhilgefort@gmail.com",
      "web": "rob.hilgefort.me"
    }
  ],
  "engines": {
    "node": ">=8"
  },
  "scripts": {
    "clean": "rm -rf node_modules",
    "reinstall": "yarn clean && yarn install",
    "lint": "esw *.js src test",
    "lint:fix": "yarn lint --fix",
    "lint:watch": "yarn lint --watch",
    "lint:staged": "lint-staged",
    "test": "jest --verbose",
    "test:watch": "yarn test --watch -o",
    "precommit": "lint-staged",
    "docs": "yarn docs:open",
    "predocs:build": "./scripts/gen-3rd-party-docs.js",
    "docs:build": "jsdoc -c jsdoc.json",
    "docs:open": "open ./docs/index.html",
    "build": "rollup -c"
  },
  "lint-staged": {
    "*.js": "esw"
  },
  "dependencies": {
    "@rjhilgefort/export-dir": "^1.1.0",
    "lodash": "^4.17.10",
    "ramda": "^0.25.0",
    "string-format": "^1.0.0",
    "stringify-object": "^3.2.1"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0-beta.49",
    "@babel/preset-env": "^7.0.0-beta.49",
    "@losant/eslint-config-losant": "^1.3.2",
    "docdash": "^0.4.0",
    "expect": "^22.4.3",
    "fs-extra": "^5.0.0",
    "handlebars": "^4.0.11",
    "husky": "^0.14.3",
    "jest": "^21.0.2",
    "jsdoc": "^3.5.5",
    "lint-staged": "^7.0.5",
    "present": "^1.0.0",
    "rollup": "^0.59.4",
    "rollup-plugin-babel": "^4.0.0-beta.4",
    "rollup-plugin-commonjs": "^9.1.3",
    "treis": "^2.5.0"
  },
  "eslintConfig": {
    "extends": "@losant/eslint-config-losant/env/node",
    "env": {
      "jest": true
    },
    "rules": {
      "arrow-body-style": 0,
      "valid-jsdoc": 0
    }
  },
  "eslintIgnore": [
    "docs"
  ]
}
