{
  "name": "losant-utils",
  "version": "1.0.6",
  "private": true,
  "main": "src/index.js",
  "repository": "git@bitbucket.org:losant/losant-utils.git",
  "author": "@Losant",
  "engines": {
    "node": "8.9.0",
    "yarn": "^1.2.1"
  },
  "scripts": {
    "preinstall": "node -e 'if(!process.env.npm_execpath.includes(`yarn`)){console.error(`Use yarn!`);process.exit(1);}'",
    "lint": "esw *.js src",
    "lint:fix": "yarn lint --fix",
    "lint:watch": "yarn lint --watch",
    "lint:staged": "lint-staged",
    "test": "jest --verbose",
    "test:watch": "yarn test --watch -o",
    "precommit": "lint-staged"
  },
  "lint-staged": {
    "*.js": "esw"
  },
  "dependencies": {
    "lodash": "^4.17.4",
    "ramda": "^0.24.1",
    "string-format": "^0.5.0",
    "stringify-object": "^3.2.1"
  },
  "devDependencies": {
    "@losant/eslint-config-losant": "^1.2.1",
    "fs-extra": "^4.0.2",
    "husky": "^0.14.3",
    "jest": "^21.0.2",
    "lint-staged": "^4.1.3"
  },
  "eslintConfig": {
    "extends": "@losant/eslint-config-losant/env/node",
    "env": {
      "jest": true
    },
    "rules": {
      "arrow-body-style": 0
    }
  }
}
