{
  "name": "omnibelt",
  "description": "A utility belt that normalizes and wraps `Ramda` and `Lodash/FP`.",
  "version": "1.3.1",
  "license": "MIT",
  "main": "src/index.js",
  "repository": "git@github.com/Losant/omnibelt.git",
  "author": "@Losant",
  "contributors": [
    {
      "name": "Rob Hilgefort",
      "email": "rjhilgefort@gmail.com",
      "web": "rob.hilgefort.me"
    }
  ],
  "engines": {
    "node": ">=8"
  },
  "scripts": {
    "reinstall": "rm -rf node_modules && yarn install",
    "lint": "esw . --ext .js",
    "lint:fix": "yarn lint --fix",
    "lint:watch": "yarn lint --watch",
    "lint:staged": "lint-staged",
    "test": "jest --verbose",
    "test:watch": "yarn test --watch -o",
    "docs": "yarn docs:open",
    "predocs:build": "./scripts/gen-3rd-party-docs.js",
    "docs:build": "jsdoc -c jsdoc.json",
    "docs:open": "open ./docs/index.html"
  },
  "lint-staged": {
    "*.js": "esw"
  },
  "dependencies": {
    "@rjhilgefort/export-dir": "^1.1.2",
    "lodash": "^4.17.11",
    "ramda": "^0.26.1",
    "string-format": "^1.0.0",
    "stringify-object": "^3.3.0"
  },
  "devDependencies": {
    "@losant/eslint-config-losant": "^1.3.5",
    "docdash": "^0.4.0",
    "expect": "^23.6.0",
    "fs-extra": "^7.0.1",
    "handlebars": "^4.0.12",
    "husky": "^1.3.1",
    "jest": "^23.6.0",
    "jsdoc": "^3.5.5",
    "lint-staged": "^8.1.0",
    "present": "^1.0.0",
    "treis": "^2.6.0"
  },
  "eslintConfig": {
    "extends": "@losant/eslint-config-losant/env/node",
    "env": {
      "jest": true
    },
    "rules": {
      "arrow-body-style": 0,
      "valid-jsdoc": 0
    }
  },
  "eslintIgnore": [
    "docs"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
