{
  "name": "losant-utils",
  "version": "1.0.12",
  "private": true,
  "main": "src/index.js",
  "repository": "git@github.com/Losant/losant-utils.git",
  "author": "@Losant",
  "engines": {
    "node": ">=8.9.4",
    "yarn": ">=1.3.2"
  },
  "scripts": {
    "preinstall": "losant-scripts force-yarn 2> /dev/null || npx @losant/scripts force-yarn",
    "reinstall": "rm -rf node_modules && yarn install",
    "lint": "esw *.js src",
    "lint:fix": "yarn lint --fix",
    "lint:watch": "yarn lint --watch",
    "lint:staged": "lint-staged",
    "test": "jest --verbose",
    "test:watch": "yarn test --watch -o",
    "precommit": "lint-staged",
    "postmerge": "losant-scripts git-hook post-merge",
    "postversion": "losant-scripts git-hook post-version"
  },
  "lint-staged": {
    "*.js": "esw"
  },
  "dependencies": {
    "@rjhilgefort/export-dir": "^0.0.4",
    "lodash": "^4.17.4",
    "ramda": "^0.24.1",
    "string-format": "^0.5.0",
    "stringify-object": "^3.2.1"
  },
  "devDependencies": {
    "@losant/eslint-config-losant": "^1.2.3",
    "@losant/scripts": "^1.0.10",
    "fs-extra": "^4.0.3",
    "husky": "^0.14.3",
    "jest": "^21.0.2",
    "lint-staged": "^6.0.0"
  },
  "eslintConfig": {
    "extends": "@losant/eslint-config-losant/env/node",
    "env": {
      "jest": true
    },
    "rules": {
      "arrow-body-style": 0,
      "valid-jsdoc": 0
    }
  }
}
